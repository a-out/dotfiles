! Urxvt ---------------

URxvt.termName:               xterm-256color
URxvt.urlLauncher:            /usr/bin/chromium
URxvt.matcher.button:         1
URxvt.borderColor:            #333631
URxvt.internalBorder:         0
URxvt*underlineColor:         #333333
URxvt*highlightColor:         #ffffff
URxvt*highlightTextColor:     #000000
URxvt.cursorColor:            #5E468C
URxvt.cursorBlink:            true
URxvt*buffered:               true
URxvt*cursorBlink:            true
URxvt*cursorUnderline:        false
URxvt*geometry: 		         92x29
URxvt*depth: 			         32
URxvt.background:             #1A1A1A
URxvt.foreground:             #999999
URxvt*transparent:		      false
URxvt*shading:			         10
URxvt*scrollBar: 		         false
URxvt*scrollBar_right:        false
URxvt*scrollWithBuffer: 	   false
URxvt*scrollTtyOutput: 		   true
URxvt*pointerBlank: 		      true

! font and colors
URxvt.font:                 xft:Sauce Code Powerline:size=10
URxvt.boldFont:             xft:Sauce Code Powerline:size=10
#include                      "/home/richie/.colors/richie"

Xft.lcdfilter:                lcddefault
Xft.hinting:                  1
Xft.hintstyle:                hintslight
Xft.antialias:                1
Xft.dpi:                      93
Xft.rgba:                     rgb

! xterm ----------------------------
xterm*toolBar: false
xterm*scrollBar: false
xterm*rightScrollBar: false
xterm*background: #1A1A1A
xterm*foreground: #A9A9A9
xterm*geometry: 80x30 
